<?xml version='1.0' encoding='utf-8'?>
<class id="iodrv::Endstop" name="Endstop" xmlns="http://jessevdk.github.com/cldoc/1.0">
  <doc>
Endstops are queriable switches placed at the axis limits.
They typically represent a "known" point to which the device can be homed upon initiailization,
  or a point beyond which the device should not be pushed.
</doc>
  <base access="public">
    <type name="IODriver" ref="iodrv::IODriver#iodrv::IODriver" />
  </base>
  <constructor id="iodrv::Endstop::Endstop" name="Endstop" />
  <constructor id="iodrv::Endstop::Endstop" name="Endstop">
    <argument id="iodrv::Endstop::Endstop::pin" name="pin">
      <type name="IoPin &amp;&amp;" />
    </argument>
  </constructor>
  <method id="iodrv::Endstop::isEndstop" name="isEndstop" override="yes">
    <return>
      <type builtin="yes" name="bool" />
    </return>
    <override name="IODriver::isEndstop" ref="iodrv::IODriver#iodrv::IODriver::isEndstop" />
  </method>
  <method id="iodrv::Endstop::isEndstopTriggered" name="isEndstopTriggered" override="yes">
    <return>
      <type builtin="yes" name="bool" />
    </return>
    <override name="IODriver::isEndstopTriggered" ref="iodrv::IODriver#iodrv::IODriver::isEndstopTriggered" />
  </method>
  <method id="iodrv::Endstop::isNull" name="isNull">
    <return>
      <type builtin="yes" name="bool" />
    </return>
  </method>
</class>
