<?xml version='1.0' encoding='utf-8'?>
<classtemplate id="motion::LinearDeltaStepper" name="LinearDeltaStepper" xmlns="http://jessevdk.github.com/cldoc/1.0">
  <doc>
LinearDeltaStepper implements the AxisStepper interface for (rail-based) Delta-style robots like the Kossel,
  for linear (G0/G1) and arc movements (G2/G3)
</doc>
  <base access="public">
    <type name="" />
  </base>
  <templatetypeparameter id="motion::LinearDeltaStepper::StepperDriverT" name="StepperDriverT" />
  <methodtemplate id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;" name="LinearDeltaStepper&lt;StepperDriverT&gt;">
    <return>
      <type builtin="yes" name="void" />
    </return>
    <argument id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;::idx" name="idx">
      <type builtin="yes" name="int" />
    </argument>
    <argument id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;::axisIdx" name="axisIdx">
      <type name="DeltaAxis" ref="motion#motion::DeltaAxis" />
    </argument>
    <argument id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;::map" name="map">
      <type name="const CoordMapT" qualifier=" const &amp;" />
    </argument>
    <argument id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;::stepper" name="stepper">
      <type name="const StepperDriverT" qualifier=" const &amp;" />
    </argument>
    <argument id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;::endstop" name="endstop">
      <type name="iodrv::Endstop" qualifier=" const *" ref="iodrv::Endstop#iodrv::Endstop" />
    </argument>
    <templatetypeparameter id="motion::LinearDeltaStepper::LinearDeltaStepper&lt;StepperDriverT&gt;::CoordMapT" name="CoordMapT" />
  </methodtemplate>
  <methodtemplate id="motion::LinearDeltaStepper::beginLine" name="beginLine">
    <brief>function to initiate a linear (through cartesian space) motion</brief>
    <return>
      <type builtin="yes" name="void" />
    </return>
    <argument id="motion::LinearDeltaStepper::beginLine::map" name="map">
      <type name="const CoordMapT" qualifier=" const &amp;" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginLine::curPos" name="curPos">
      <type name="std::array&lt;typename, std::size_t&gt;" qualifier=" const &amp;" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginLine::vel" name="vel">
      <type name="Vector4f" qualifier=" const &amp;" ref="index#Vector4f" />
    </argument>
    <templatetypeparameter id="motion::LinearDeltaStepper::beginLine::CoordMapT" name="CoordMapT" />
    <templatenontypeparameter default="std" id="motion::LinearDeltaStepper::beginLine::sz" name="sz">
      <type name="std::size_t" />
    </templatenontypeparameter>
  </methodtemplate>
  <methodtemplate id="motion::LinearDeltaStepper::beginArc" name="beginArc">
    <brief>function to initiate a circular arc (through cartesian space) motion</brief>
    <return>
      <type builtin="yes" name="void" />
    </return>
    <argument id="motion::LinearDeltaStepper::beginArc::map" name="map">
      <type name="const CoordMapT" qualifier=" const &amp;" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::curPos" name="curPos">
      <type name="std::array&lt;typename, std::size_t&gt;" qualifier=" const &amp;" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::center" name="center">
      <type name="Vector3f" qualifier=" const &amp;" ref="index#Vector3f" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::u" name="u">
      <type name="Vector3f" qualifier=" const &amp;" ref="index#Vector3f" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::v" name="v">
      <type name="Vector3f" qualifier=" const &amp;" ref="index#Vector3f" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::arcRad" name="arcRad">
      <type builtin="yes" name="float" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::arcVel" name="arcVel">
      <type builtin="yes" name="float" />
    </argument>
    <argument id="motion::LinearDeltaStepper::beginArc::extVel" name="extVel">
      <type builtin="yes" name="float" />
    </argument>
    <templatetypeparameter id="motion::LinearDeltaStepper::beginArc::CoordMapT" name="CoordMapT" />
    <templatenontypeparameter default="std" id="motion::LinearDeltaStepper::beginArc::sz" name="sz">
      <type name="std::size_t" />
    </templatenontypeparameter>
  </methodtemplate>
  <method id="motion::LinearDeltaStepper::testDir" name="testDir">
    <brief>protected:</brief>
    <return>
      <type builtin="yes" name="float" />
    </return>
    <argument id="motion::LinearDeltaStepper::testDir::s" name="s">
      <type builtin="yes" name="float" />
    </argument>
  </method>
  <method id="motion::LinearDeltaStepper::_nextStep" name="_nextStep">
    <return>
      <type builtin="yes" name="void" />
    </return>
    <argument id="motion::LinearDeltaStepper::_nextStep::useEndstops" name="useEndstops">
      <type builtin="yes" name="bool" />
    </argument>
  </method>
</classtemplate>
