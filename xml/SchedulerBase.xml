<?xml version='1.0' encoding='utf-8'?>
<class id="SchedulerBase" interface="true" name="SchedulerBase" xmlns="http://jessevdk.github.com/cldoc/1.0">
  <doc>
SchedulerBase allows any file to insert exit handlers, without adding the entire Scheduler as a dependency.
</doc>
  <subclass access="public" name="Scheduler" ref="Scheduler#Scheduler" />
  <method abstract="yes" id="SchedulerBase::configureExitHandlers" name="configureExitHandlers" static="yes">
    <return>
      <type builtin="yes" name="void" />
    </return>
  </method>
  <method abstract="yes" id="SchedulerBase::registerExitHandler" name="registerExitHandler" static="yes">
    <brief>need to return a value so we can do tricks like `static bool _wasInit=registerExitHandler(...)` to do something just once</brief>
    <return>
      <type builtin="yes" name="bool" />
    </return>
    <argument id="SchedulerBase::registerExitHandler::handler" name="handler">
      <type class="function" qualifier="*">
        <result>
          <type builtin="yes" name="void" />
        </result>
        <arguments />
      </type>
    </argument>
    <argument id="SchedulerBase::registerExitHandler::level" name="level">
      <type builtin="yes" name="unsigned int" />
    </argument>
  </method>
</class>
